    <url-pattern>/client.jsp</url-pattern>
    <url-pattern>/contactUs.jsp</url-pattern>
    <url-pattern>/deposits.jsp</url-pattern>
    <url-pattern>/depositToAccount.jsp</url-pattern>
    <url-pattern>/error.jsp</url-pattern>
    <url-pattern>/main.jsp</url-pattern>
